<%= form_for(page) do |f| %>
<head>
  <link rel="stylesheet" href="/style.css">
</head>
  <div class="field">
    <%= f.label :tittle %>
    <%= f.text_field :tittle %>
  </div>

  <div class="field">
    <%= f.label :body %>
    <%= f.text_area :body %>
  </div>

  <div class="field">
    <%= f.label :slug %>
    <%= f.text_area :slug %>
  </div>


  <div>
    <%= f.submit %>
  </div>
<% end %>